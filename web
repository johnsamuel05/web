<script language="javascript">
  
function validate(){  
    var name=document.frm1.nam1.value; 
    var rno= document.frm1.regno.value;
    var mobile=document.frm1.mp.value;  
      
    if (name==null || name=="")  
      alert("Name can't be blank");  
     
     if (rno==null || rno=="")  
      alert("regno can't be blank");  
       
     if(mobile.length!=10)  
      alert("mobile number must be 10 digits  long."); 
 
      if (/^[a-zA-Z0-9.!#$%&'+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)$/.test(frm1.em.value))
  {
    return (true);
  }
    alert("You have entered an invalid email address!");
   }  
</script>  
 <title>Page Title</title>
</head>
