<?php
        $dbhost='localhost';
        $dbuser='root';
        $dbpass='';
      $dbname='Hope';

$conn = new mysqli($dbhost, $dbuser, $dbpass, $dbname);
// Check connection
if ($conn->connect_error) {
  die("Connection failed: " . $conn->connect_error);
}
else
echo "Connected successfully now";

$r=$_REQUEST['regno'];
$n=$_REQUEST['nam1'];
$m=$_REQUEST['mp'];
$e=$_REQUEST['em'];

$sql = "insert into student values ($r,'$n',$m,'$e')";

if ($conn->query($sql) === TRUE) {
  echo "<br> New record created successfully";
} else {
  echo "Error: " . $sql . "<br>" . $conn->error;
}

$conn->close();
?>

